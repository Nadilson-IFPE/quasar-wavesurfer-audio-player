<?xml version='1.0' encoding='utf-8'?>
<widget id="com.nadilson.quasarapp" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Cool Player by Nadilson</name>
    <description>A SoundCloud-like audio player using Quasar and Wavesurfer</description>
    <author email="dev@cordova.apache.org" href="https://cordova.apache.org">
        Apache Cordova Team
  </author>
    <content src="index.html" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-navigation href="about:*" />
    <platform name="ios">
        <preference name="ScrollEnabled" value="true" />
    </platform>
    <preference name="Orientation" value="portrait" />
</widget>
